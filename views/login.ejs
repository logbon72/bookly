<h1>Login</h1>
<form ng-controller="AuthController" ng-submit="login()">
  <label for="email">Email</label> <input ng-model="email" type="email" name="email" id="email"><br><br>
  <label for="password">Password</label> <input ng-model="password" type="password" name="password" id="password"><br><br>
  <input type="submit" value="submit">
</form>
<section><a href="register" title="Register">...or Register</a></section>
